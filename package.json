{
    "name": "vue-todo",
    "description": "A Vue.js project",
    "version": "1.0.0",
    "author": "Tremus <tredudman@yahoo.com.au>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "concurrently -n http,web -c white,cyan 'npm run dev:http' 'npm run dev:webpack'",
        "dev:webpack": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
        "dev:http": "nodemon -w src/server.js src/server.js",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
    },
    "dependencies": {
        "concurrently": "^5.3.0",
        "sequelize": "^6.3.5",
        "sequelize-cli": "^6.2.0",
        "sqlite3": "^5.0.0",
        "vue": "^2.5.11",
        "vue-head": "^2.2.0",
        "vue-router": "^3.4.3"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-3": "^6.24.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "file-loader": "^1.1.4",
        "vue-loader": "^13.0.5",
        "vue-template-compiler": "^2.4.4",
        "webpack": "^3.6.0",
        "webpack-dev-server": "^2.9.1"
    }
}
